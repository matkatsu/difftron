{
  "presets": [
    [
      "flow"
    ],
    [
      "react"
    ],
    [
      "env",
      {
        "targets": {
          "electron": 1.4
        }
      }
    ]
  ],
  "plugins": [
    [
      "module-resolver",
      {
        "root": [
          "./App"
        ]
      }
    ]
  ],
  "env": {
    "production": {
      "presets": [
        "react-optimize"
      ],
      "plugins": [
        "babel-plugin-dev-expression"
      ]
    },
    "development": {
      "presets": [
        "react-hmre"
      ]
    }
  }
}