{
  "name": "difftron",
  "productName": "Difftron",
  "version": "1.0.0",
  "description": "diff tool based on Electron",
  "main": "main.js",
  "scripts": {
    "dev": "concurrently --kill-others \"npm run hot-server\" \"npm run start-hot\"",
    "hot-server": "cross-env NODE_ENV=development node -r babel-register server.js",
    "start-hot": "cross-env HOT=1 NODE_ENV=development electron -r babel-register -r babel-polyfill ./main.development",
    "test": "ava",
    "test:watch": "ava --watch",
    "coverage": "nyc ava && nyc report --reporter=html && open coverage/index.html || xdg-open coverage/index.html",
    "build-main": "cross-env NODE_ENV=production node -r babel-register ./node_modules/webpack/bin/webpack --config webpack.config.electron.js --progress --profile --colors",
    "build-renderer": "cross-env NODE_ENV=production node -r babel-register ./node_modules/webpack/bin/webpack --config webpack.config.production.js --progress --profile --colors",
    "build": "npm run build-main && npm run build-renderer",
    "start": "cross-env NODE_ENV=production electron ./",
    "package": "cross-env NODE_ENV=production node -r babel-register -r babel-polyfill package.js",
    "package-all": "npm run package -- --all"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/romiogaku/difftron.git"
  },
  "keywords": [
    "Electron",
    "diff"
  ],
  "author": "romiogaku",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/romiogaku/difftron/issues"
  },
  "homepage": "https://github.com/romiogaku/difftron#readme",
  "devDependencies": {
    "asar": "^0.13.0",
    "ava": "^0.18.1",
    "babel-core": "^6.23.1",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.3.2",
    "babel-plugin-dev-expression": "^0.2.1",
    "babel-plugin-webpack-loaders": "^0.8.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-react-optimize": "^1.0.1",
    "babel-preset-stage-0": "^6.22.0",
    "babel-register": "^6.23.0",
    "concurrently": "^3.3.0",
    "cross-env": "^3.1.4",
    "css-loader": "^0.26.1",
    "del": "^2.2.2",
    "devtron": "^1.4.0",
    "electron": "^1.4.15",
    "electron-devtools-installer": "^2.1.0",
    "electron-packager": "^8.5.1",
    "electron-rebuild": "^1.5.7",
    "eslint": "^3.15.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-formatter-pretty": "^1.1.0",
    "eslint-import-resolver-webpack": "^0.8.1",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-flowtype": "^2.30.0",
    "eslint-plugin-flowtype-errors": "^3.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-mocha": "^4.8.0",
    "eslint-plugin-promise": "^3.4.2",
    "eslint-plugin-react": "^6.10.0",
    "express": "^4.14.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "flow-bin": "^0.39.0",
    "json-loader": "^0.5.4",
    "minimist": "^1.2.0",
    "nyc": "^10.1.2",
    "proxyquire": "^1.7.11",
    "reactotron-react-js": "^1.7.0",
    "reactotron-redux": "^1.7.0",
    "reactotron-redux-saga": "^1.7.0",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.2",
    "webpack-dev-middleware": "^1.8.4",
    "webpack-hot-middleware": "^2.13.0",
    "webpack-merge": "^0.15.0",
    "webpack-validator": "^2.2.9"
  },
  "dependencies": {
    "diff": "^3.0.1",
    "diff2html": "^2.0.10",
    "electron-debug": "^1.0.1",
    "localforage": "^1.4.3",
    "react": "^15.3.2",
    "react-ace": "^3.6.1",
    "react-dom": "^15.3.2",
    "react-redux": "^4.4.5",
    "react-split-pane": "^0.1.52",
    "redux": "^3.6.0",
    "redux-persist": "^3.5.0",
    "redux-saga": "^0.12.1",
    "reduxsauce": "^0.2.0",
    "seamless-immutable": "^6.3.0",
    "source-map-support": "^0.4.6"
  },
  "ava": {
    "files": [
      "Tests/**/*.js",
      "!Tests/Setup.js"
    ],
    "require": [
      "babel-register",
      "babel-polyfill"
    ],
    "babel": "inherit"
  }
}